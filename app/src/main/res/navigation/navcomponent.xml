<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navcomponen"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.sleepydev.bobosa.View.Fragment.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.sleepydev.bobosa.View.Fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
        <action
            android:id="@+id/action_homeFragment_to_libFragment"
            app:destination="@id/libFragment" />
        <action
            android:id="@+id/action_homeFragment_to_panduanFragment"
            app:destination="@id/panduanFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.sleepydev.bobosa.View.Fragment.ResultFragment"
        android:label="ResultFragment" />
    <fragment
        android:id="@+id/libFragment"
        android:name="com.sleepydev.bobosa.View.Fragment.LibFragment"
        android:label="LibFragment" />
    <fragment
        android:id="@+id/panduanFragment"
        android:name="com.sleepydev.bobosa.View.Fragment.PanduanFragment"
        android:label="PanduanFragment" />
</navigation>